
{
    "version": 2,
    "builds": [
      {
        "src": "src/index.js",
        "use": "@vercel/node-server"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "src/index.js"
      }
    ],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/:match*"
      }
    ]
  }
  